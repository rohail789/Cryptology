@model IEnumerable<Cryptology.Currency>
@{
    ViewBag.Title = "Home Page";
}
<div class="container">
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner">
            <div class="item active">
                <img src="~/Content/images/1img.jpg" alt="" style="width:100%;">
                <div class="carousel-caption">
                    <h3>Coins</h3>
                    <p>it will be your's if u really want.</p>
                </div>
            </div>

            <div class="item">
                <img src="~/Content/images/2img.png" alt="" style="width:100%;">
                <div class="carousel-caption">
                    <h3>Wowww</h3>
                    <p>i cant believe it's mine.</p>
                </div>
            </div>

            <div class="item">
                <img src="~/Content/images/_bg.jpeg" alt="" style="width:100%;">
                <div class="carousel-caption">
                    <h3>Money</h3>
                    <p>oh my god....</p>
                </div>
            </div>
        </div>

        <!-- Left and right controls -->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>
<div style="width: 100%;height: 10px;"></div>

<div class="table-responsive">
    <table class="table">
        <thead>
            <tr>
                <th>Name</th>
                <th>Price_Usd</th>
                <td>Symbol</td>
                <td>Rank</td>
                <td>PriceBTC</td>
                <td>24h_volume_usd</td>
                <td>Market_Cap_usd</td>
                <td>Available_Supply</td>
                <td>Max_Supply</td>
                <td>Percent_Change_7d</td>
                <td>Last_Update</td>
            </tr>
        </thead>
         
    @foreach (var item in Model)
    {
        <tbody id="tbt">
            <tr class="success">
                <td><a href="https://coinmarketcap.com">
                    @Html.DisplayFor(modelItem => item.Cname)
                       </a>
                </td>
                <td><a href="https://coinmarketcap.com">
                    $@Html.DisplayFor(modelItem => item.Price_usd)
                    </a>
                </td>
                <td><a href="https://coinmarketcap.com">
                    @Html.DisplayFor(modelItem => item.Symbol)
                    </a>
                </td>
                <td><a href="https://coinmarketcap.com">
                    @Html.DisplayFor(modelItem => item.Rank)
                    </a>
                </td>
                <td><a href="https://coinmarketcap.com">
                    @Html.DisplayFor(modelItem => item.Price_btc)
                    </a>
                </td>
                <td><a href="https://coinmarketcap.com">
                    $@Html.DisplayFor(modelItem => item.C24h_volume_usd)
                    </a>
                </td>
                <td><a href="https://coinmarketcap.com">
                    $@Html.DisplayFor(modelItem => item.Market_cap_usd)
                    </a>
                </td>
                <td><a href="https://coinmarketcap.com">
                    @Html.DisplayFor(modelItem => item.Available_supply)
                    </a>
                </td>
                <td><a href="https://coinmarketcap.com">
                    @Html.DisplayFor(modelItem => item.Max_supply)
                    </a>
                </td>
                <td><a href="https://coinmarketcap.com">
                    @Html.DisplayFor(modelItem => item.Percent_change_7d)%
                    </a>
                </td>
                <td><a href="https://coinmarketcap.com">
                    @Html.DisplayFor(modelItem => item.Last_updated)
                    </a>
                </td>

            </tr>

        </tbody>
    }
    </table>

</div>
<div style="width: 100%;height: 10px;"></div>
<div class="container">
   
</div>

